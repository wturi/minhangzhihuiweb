@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="row">
    <div class="col-lg-2"></div>
    <div class="col-lg-8">
        <ul id="myTab" class="nav nav-tabs">
            <li class="active"><a href="#zhengshi" data-toggle="tab">正式出入园</a></li>
            <li><a href="#linshi" data-toggle="tab">临时进园区</a></li>
        </ul>
        <div id="myTabContent" class="tab-content">
            <div class="tab-pane fade in active" id="zhengshi">
                <div class="input-group">
                    <input type="text" name="q" class="form-control" placeholder="搜索..." />
                    <span class="input-group-btn">
                        <button type='submit' name='search' typename="check" typenumber="1" class="btn btn-flat search-btn"><i class="fa fa-search"></i></button>
                    </span>
                </div>
                <table class="table table-striped table-hover text-center" id="table1">
                    <thead>
                        <tr>
                            <th style="width:55%">申请标题</th>
                            <th style="width:25%">申请时间</th>
                            <th style="width:20%">审核状态</th>
                        </tr>
                    </thead>
                    <tbody>
                        @if (ViewBag.CheckList1 != null)
                        {
                            foreach (MinHangWisdomParkWeb.Controllers.CheckInOutList a in ViewBag.CheckList1 as List<MinHangWisdomParkWeb.Controllers.CheckInOutList>)
                            {
                                <tr>
                                    <td><button type="button" class="btn btn-link" data-toggle="modal" data-target="#modelxiangxi" typename="check" applyid="@a.CheckID" statetype="zhengshi">@a.Title</button></td>
                                    <td>@a.Time</td>
                                    <td>@a.StatusName</td>
                                </tr>
                            }
                        }
                    </tbody>
                </table>
                <nav class="pull-right">
                    <ul class="pagination">
                        <li>
                            <a href="#" aria-label="Previous" class="ListPrevious" id="paginpre1">
                                <span aria-hidden="true">&laquo;</span>
                            </a>
                        </li>

                        <li>
                            <a href="#" aria-label="Next" class="ListNext">
                                <span aria-hidden="true">&raquo;</span>
                            </a>
                        </li>
                    </ul>
                </nav>
            </div>
            <div class="tab-pane fade" id="linshi">
                <div class="input-group">
                    <input type="text" name="q" class="form-control" placeholder="搜索..." />
                    <span class="input-group-btn">
                        <button type='submit' name='search' typename="check" typenumber="2" class="btn btn-flat search-btn"><i class="fa fa-search"></i></button>
                    </span>
                </div>
                <table class="table table-striped table-hover text-center" id="table2">
                    <thead>
                        <tr>
                            <th style="width:55%">申请标题</th>
                            <th style="width:25%">申请时间</th>
                            <th style="width:20%">审核状态</th>
                        </tr>
                    </thead>
                    <tbody>
                        @if (ViewBag.CheckList2 != null)
                        {
                            foreach (MinHangWisdomParkWeb.Controllers.CheckInOutList a in ViewBag.CheckList2 as List<MinHangWisdomParkWeb.Controllers.CheckInOutList>)
                            {
                                <tr>
                                    <td><button type="button" class="btn btn-link" data-toggle="modal" data-target="#modelxiangxi" typename="check" applyid="@a.CheckID" statetype="lingshi">@a.Title</button></td>
                                    <td>@a.Time</td>
                                    <td>@a.StatusName</td>
                                </tr>
                            }
                        }
                    </tbody>
                </table>
                <nav class="pull-right">
                    <ul class="pagination">
                        <li>
                            <a href="#" aria-label="Previous" class="ListPrevious" id="paginpre2">
                                <span aria-hidden="true">&laquo;</span>
                            </a>
                        </li>

                        <li>
                            <a href="#" aria-label="Next" class="ListNext">
                                <span aria-hidden="true">&raquo;</span>
                            </a>
                        </li>
                    </ul>
                </nav>
            </div>
        </div>
    </div>
    <div class="col-lg-2"></div>
</div>

<!-- Modal -->
<div class="modal fade" id="modelxiangxi" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="myModalLabel"></h4>
            </div>
            <div class="modal-body" id="myModelContent">

            </div>
        </div>
    </div>
</div>
<input type="hidden" id="BuniessNumber1" value="@ViewBag.CheckNumber1" />
<input type="hidden" id="BuniessNumber2" value="@ViewBag.CheckNumber2" />
<script src="~/ViewScripts/BuniessApplication-InOrOutAll.js"></script>
<script src="~/ViewScripts/fanye.js"></script>