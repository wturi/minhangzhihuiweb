@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<style type="text/css">
    .jumbotron {
        background-color: white;
    }
</style>
<div class="row">
    <div class="col-md-2"></div>
    <div class="col-md-8">
        <ul id="myTab" class="nav nav-tabs">
            <li class="active">
                <a href="#People" data-toggle="tab">正式出入园区</a>
            </li>
            <li><a href="#ka" data-toggle="tab">临时出入园区</a></li>
        </ul>
        <div id="myTabContent" class="tab-content">
            <!--人员制卡申请-->
            <div class="tab-pane fade in active" id="People">

                <div class="input-group">
                    <input type="text" name="q" class="form-control" placeholder="搜索..." />
                    <span class="input-group-btn">
                        <button type='submit' name='search' tag="0" typename="buniess" typenumber="4" class="btn btn-flat  search-btn"><i class="fa fa-search"></i></button>
                    </span>
                </div>

                <table class="table table-striped table-hover text-center" id="table4">
                    <thead>
                        <tr>
                            <th style="width:20%;">标题</th>
                            <th style="width:20%;">申请人</th>
                            <th style="width:20%">所属业主</th>
                            <th style="width:20%;">申请时间</th>
                            <th style="width:20%;">操作</th>
                        </tr>
                    </thead>
                    <tbody>
                        @if (ViewBag.CheckList1 != null)
                        {
                            foreach (MinHangWisdomParkWeb.Controllers.ShenHe b in ViewBag.CheckList1 as List<MinHangWisdomParkWeb.Controllers.ShenHe>)
                            {
                                <tr>
                                    <td><button type="button" class="btn btn-link buniesstitle" data-toggle="modal" data-target="#modelxiangxi" applyid="@b.Content" statetype="zhengshi">@b.Title</button></td>
                                    <td>@b.Updater</td>
                                    <td>@b.ownerid</td>
                                    <td>@b.Time</td>
                                    <td>
                                        <div class="btn-group" role="group" tag="@b.ApplyID">
                                            <button type="button" class="btn btn-info tongguo">通过</button>
                                            <button type="button" class="btn btn-danger butongguo">不通过</button>
                                        </div>
                                    </td>
                                </tr>
                            }
                        }
                    </tbody>
                </table>

                <nav class="pull-right">
                    <ul class="pagination">
                        <li>
                            <a href="#" aria-label="Previous" class="ListPrevious" id="paginpre4">
                                <span aria-hidden="true">&laquo;</span>
                            </a>
                        </li>

                        <li>
                            <a href="#" aria-label="Next" class="ListNext">
                                <span aria-hidden="true">&raquo;</span>
                            </a>
                        </li>
                    </ul>
                </nav>
            </div>

            <!--证卡挂失-->
            <div class="tab-pane fade" id="ka">
                <div class="input-group">
                    <input type="text" name="q" class="form-control" placeholder="搜索..." />
                    <span class="input-group-btn">
                        <button type='submit' name='search' tag="1" typename="buniess" typenumber="5" class="btn btn-flat search-btn"><i class="fa fa-search"></i></button>
                    </span>
                </div>
                <table class="table table-striped table-hover text-center" id="table5">
                    <thead>
                        <tr>
                            <th style="width:20%">标题</th>
                            <th style="width:20%">申请人</th>
                            <th style="width:20%">所属业主</th>
                            <th style="width:20%">申请时间</th>
                            <th style="width:20%">操作</th>
                        </tr>
                    </thead>
                    <tbody>
                        @if (ViewBag.CheckList2 != null)
                        {
                            foreach (MinHangWisdomParkWeb.Controllers.ShenHe b in ViewBag.CheckList2 as List<MinHangWisdomParkWeb.Controllers.ShenHe>)
                            {
                                <tr>
                                    <td><button type="button" class="btn btn-link buniesstitle" data-toggle="modal" data-target="#modelxiangxi" applyid="@b.Content" statetype="lingshi">@b.Title</button></td>
                                    <td>@b.Updater</td>
                                    <td>@b.ownerid</td>
                                    <td>@b.Time</td>
                                    <td>
                                        <div class="btn-group" role="group" tag="@b.ApplyID">
                                            <button type="button" class="btn btn-info tongguo">通过</button>
                                            <button type="button" class="btn btn-danger butongguo">不通过</button>
                                        </div>
                                    </td>
                                </tr>
                            }
                        }
                    </tbody>
                </table>
                <nav class="pull-right">
                    <ul class="pagination">
                        <li>
                            <a href="#" aria-label="Previous" class="ListPrevious" id="paginpre5">
                                <span aria-hidden="true">&laquo;</span>
                            </a>
                        </li>
                        <li>
                            <a href="#" aria-label="Next" class="ListNext">
                                <span aria-hidden="true">&raquo;</span>
                            </a>
                        </li>
                    </ul>
                </nav>
            </div>

        </div>
    </div>
    <div class="col-md-2"></div>
</div>

<!-- Modal -->
<div class="modal fade" id="modelxiangxi" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="myModalLabel"></h4>
            </div>
            <div class="modal-body" id="myModelContent">

            </div>
        </div>
    </div>
</div>
<input type="hidden" id="BuniessNumber4" value="@ViewBag.BuniessNumber[0]" />
<input type="hidden" id="BuniessNumber5" value="@ViewBag.BuniessNumber[1]" />

<script src="~/ViewScripts/AuthorizationAudit-InOrOut.js"></script>
<script src="~/ViewScripts/fanye.js"></script>